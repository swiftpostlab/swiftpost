    "@mui/icons-material": "^5.14.12",
    "@mui/material": "^5.11.2",
    "@next/mdx": "^13.1.6",
    "@types/node": "20.5.9",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "eslint": "8.48.0",
    "eslint-config-next": "13.1.5",
    "next": "13.4.19",
    "next-mdx-remote": "^4.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "remark-gfm": "^3.0.1",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.47.1",
