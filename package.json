{
  "name": "swiftpost",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --project tsconfig.json",
    "mock": "(rm ./config -rf || true) && withArgs(){ cp -r \"./mocks/$1/config\" ./config; }; withArgs",
    "hash": "yarn-${{ hashFiles('yarn.lock') }}s",
    "install-immutable": "yarn install --immutable --immutable-cache --check-cache"
  },
  "dependencies": {
